<table cellpadding="0" cellspacing="0" class="list">
	<colgroup>
		<col style="width:35%;"/>
		<col />
	</colgroup>
	<tr>
	  <th>Name</th>
	  <td><%=h address.first_name %> <%=h address.last_name %></td>
	</tr>
  <tr>
    <th>Telephone</th>
    <td><%=h address.telephone %></td>
  </tr>
	<tr>
	  <th>Address</th>
	  <td><%=h address.address %></td>
	</tr>
	<tr>
	  <th>City</th>
	  <td><%=h address.city %></td>
	</tr>
	<tr>
	  <th>State</th>
	  <td><%=h address.state %></td>
	</tr>
	<tr>
	  <th>Zip</th>
	  <td><%=h address.zip %></td>
	</tr>
  <tr>
	  <th>Country</th>
	  <td><%=h address.country.name %></td>
	</tr>

</table>
